											<textbox id="folder_precedence_m" size="1" maxlength="1" type="number" min="1" max="4" tooltiptext="&precedence_tooltip;" />
										</row>
										<row align="center">
											<label control="livemark_precedence_m" value="&livemark_precedence;" />
