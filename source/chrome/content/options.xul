											<hbox>
												<checkbox id="reverse_sort_m0" label="&reverse_sort;" tooltiptext="&reversesort_tooltip;" />
												<checkbox id="case_insensitive_m0" label="&case_insensitive;" tooltiptext="&caseinsensitive_tooltip;" />
											</hbox>
										</groupbox>
									</tabpanel>
